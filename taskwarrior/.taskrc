# [Created by task 2.5.3 4/1/2021 10:12:06]
# Taskwarrior program configuration file.
# For more documentation, see http://taskwarrior.org or try 'man task', 'man task-color',
# 'man task-sync' or 'man taskrc'

# Use the command 'task show' to see all defaults and overrides

# Files
data.location=~/.task

# Dates
weekstart=Monday

# Filters
search.case.sensitive=no

# Aliases
alias.a=add
alias.@=context
alias.sch=scheduled
alias.s=long scheduled

# Context
context.home.read=-work and proj.not:vipps proj.not:arch
context.home.write=
context.arch.read=proj:arch
context.arch.write=proj:arch
context.raja.read=proj:arch.lang.raja
context.raja.write=proj:arch.lang.raja
context.work.read=+work
context.work.write=+work
context.vipps.read=proj:vipps or +work
context.vipps.write=proj:vipps
context.hero.read=pro:vipps.hero
context.hero.write=pro:vipps.hero

# Notes
uda.note.type=string
uda.note.label=Note
uda.note.values=,s,l

# Custom reports

# simple
# for pi
report.simple.description=Simple list
report.simple.columns=id,description.count,project
report.simple.labels=Id,Description,project
report.simple.filter=+READY
report.simple.sort=urgency+

# Maybe
# tasks that I want to work on someday, but have no plan on how or when
# (wait:later)
report.maybe.description=List of tasks to MAYBE do
report.maybe.columns=id,project,description.count,tags,entry.age,note
report.maybe.labels=ID,Project,Description,Tags,Age,Note
report.maybe.filter=status:pending +maybe
report.maybe.dateformat=a, d b

# Later
# Tasks that needs to be done later, but I have not scheduled a specific date for them yet
# (sch:later)
report.later.description=List of tasks to do later
report.later.columns=id,entry.age,depends.indicator,priority,project,tags,recur.indicator,scheduled.countdown,due.countdown,until.remaining,description,note,urgency.integer
report.later.labels=ID,Age,D,P,Project,Tags,R,S,Due,Until,Description,Note,Urg
report.later.filter=status:pending scheduled.after:3000-12-30
report.later.dateformat=a, d b
report.later.sort=urgency-

# Todo
# All tasks that is not completed
report.todo.description=List of all tasks that is not completed
report.todo.columns=id,entry.age,depends.indicator,priority,project,tags,recur.indicator,scheduled.countdown,due.countdown,until.remaining,description,note,urgency.integer
report.todo.labels=ID,Age,D,P,Project,Tags,R,S,Due,Until,Description,Note,Urg
report.todo.filter=status.not:completed status.not:deleted
report.todo.dateformat=a, d b
report.todo.sort=urgency-


# Inbox
# tasks that need to be handled
# no project
report.inbox.description=Inbox
report.inbox.columns=id,entry.age,project,tags,description,note
report.inbox.labels=ID,Age,Project,Tags,Description,Note
report.inbox.filter=status:pending (project: )
report.inbox.dateformat=a, d b


# Plan
# Tasks that have a plan
# (sch<later)
report.plan.description=List of planned tasks
report.plan.columns=id,project,description.count,scheduled,scheduled.remaining,due.remaining,urgency.integer
report.plan.labels=Id,Project,Description,Scheduled,In,Due,Urg
report.plan.sort=scheduled+
report.plan.filter=status:pending scheduled.before:3000-12-30
report.plan.dateformat=a, d b

# Modify Ready report
report.ready.labels=ID,D,P,Project,Description,Sch,Tags,R,Until,Note,Due,Urg
report.ready.columns=id,depends.indicator,priority,project,description.count,scheduled,tags,recur.indicator,until.remaining,note,due.relative,urgency.integer
report.ready.filter=+READY -maybe -milestone
report.ready.dateformat=a, d


# Modify next report
report.next.columns=id,start.age,priority,project,tags,description.count,recur,scheduled.formatted,due.relative,until.remaining,depends,note,urgency.integer
report.next.labels=ID,Active,P,Project,Tag,Description,Recur,S,Due,Until,Deps,Note,Urg
report.next.filter=status:pending
report.next.dateformat=d b

# Modify all report (project column)
report.all.columns=id,status.short,uuid.short,start.active,entry.age,end.age,depends.indicator,priority,project,tags.count,recur.indicator,wait.remaining,scheduled.remaining,due,until.remaining,description


# Urgency
urgency.user.tag.fun.coefficient=-2.0
urgency.user.tag.maybe.coefficient=-10.0
urgency.user.tag.waiting.coefficient=-8.0
urgency.user.tag.milestone.coefficient=-10.0
urgency.user.tag.next.coefficient=10.0
urgency.user.project.vipps.hero.coefficient=4.0
urgency.user.project.familie.coefficient=8.0

calendar.details=full
calendar.holidays=full

# Priorities
uda.priority.values=H,M,,L
urgency.uda.priority.L.coefficient=6
urgency.uda.priority.M.coefficient=3
urgency.uda.priority..coefficient=0
urgency.uda.priority.L.coefficient=-6

# Color Theme
rule.precedence.color=deleted,completed,due.today,due,active,uda.priority,tag.next,keyword.,overdue,blocked,blocking,scheduled.today,scheduled,recurring,tag.,project.,tagged,uda.

# Due
color.due=color13
color.due.today=green
color.overdue=red

# General decoration
color.label=
color.label.sort=
color.alternate=
color.header=color4
color.footnote=color6
color.warning=color0 on color3
color.error=color6
color.debug=color6
color.tag.maybe=color13
color.tag.milestone=color0 on color13


# Background colors
color.active=white on color23
color.blocked=on color59
color.blocking=on color0
#color.blocked=
#color.blocking=

## Task state
color.completed=green
color.deleted=red
#color.active=color6
color.recurring=color4
color.until=

# Project
color.project.none=

# color.keyword.PR=color176
# color.keyword.fix=color103

# Priority
color.uda.priority.H=white on color52
color.uda.priority.M=color3
color.uda.priority.=
color.uda.priority.L=color102 # grey

# Tags
color.tag.next=green
color.tag.none=
color.tag.waiting=color102
color.tagged=

# color.scheduled=color110
color.scheduled=

# Report: burndown
color.burndown.done=color0 on color2
color.burndown.pending=color0 on color1
color.burndown.started=color0 on color3

# Report: history
color.history.add=color0 on color1
color.history.delete=color0 on color3
color.history.done=color0 on color6

# Report: summary
color.summary.background=on color0
color.summary.bar=color0 on color6

# Command: calendar
color.calendar.due=color0 on color9
color.calendar.due.today=color0 on color1
color.calendar.holiday=
color.calendar.overdue=color0 on color5
color.calendar.today=color0 on color4
color.calendar.weekend=
color.calendar.weeknumber=color4
color.sync.added=color14
color.sync.changed=color9
color.sync.rejected=color13


# Taskwarrior tui
uda.taskwarrior-tui.calendar.months-per-row=4
uda.taskwarrior-tui.task-report.next.filter=+READY
uda.taskwarrior-tui.task-report.prompt-on-delete=true
uda.taskwarrior-tui.task-report.prompt-on-done=true
# uda.taskwarrior-tui.keyconfig.shortcut1=n
uda.taskwarrior-tui.shortcuts.1=tblock
uda.taskwarrior-tui.shortcuts.2=tsub

# Remove context
verbose=affected,blank,edit,header,footnote,label,new-id,project,special,sync,override,recur

# Command: undo
color.undo.after=color2
color.undo.before=color1
news.version=2.6.0
context=work
